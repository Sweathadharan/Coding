#include<stdio.h>
#include<math.h>
int main()
{
    int n,rem=0,sum=0,temp;
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        rem=n%10;
        sum=sum+pow(rem,3);
        n=n/10;
        
    }
    if(sum==temp)
    {
        printf("Armstrong");
    }
    else
    {
        printf("Not a Armstrong");
    }
}
